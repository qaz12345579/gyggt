-- 서버 스크립트 디컴파일 시도
local function decompileScript(script)
    local success, result = pcall(function()
        return script.Source  -- 대부분 nil (서버에만 있음)
    end)
    
    -- 대신 RemoteFunction 처리 함수 후킹 시도
    for _, v in pairs(getgc()) do
        if type(v) == "function" then
            local info = debug.getinfo(v)
            if info.source and info.source:find("ServerScript") then
                print("찾음:", v)
                print(debug.getproto(v))  -- 프로토 출력
            end
        end
    end
end
